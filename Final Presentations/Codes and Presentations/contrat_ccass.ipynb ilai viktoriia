   "source": [
    "### Récupération des numéros d'articles du droit des contrats"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "url_contrat = \"https://www.legifrance.gouv.fr/codes/section_lc/LEGITEXT000006070721/LEGISCTA000032006712/\"\n",
    "response = requests.get(url_contrat)\n",
    "soup = BeautifulSoup(response.text, 'html.parser')\n",
    "\n",
    "codes = []\n",
    "for a in soup.find_all('p', class_='name-article'):\n",
    "    article = a.find('a')\n",
    "    if article:\n",
    "        codes.append(article.text.split(' ')[1])"
   ]
  },
